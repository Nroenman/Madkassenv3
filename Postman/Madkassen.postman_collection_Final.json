		{
			"name": "Update Product",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200 or 204\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([200, 204]);\r",
							"});\r",
							"\r",
							"pm.test(\"Response is JSON\", function () {\r",
							"    pm.response.to.have.header(\"Content-Type\");\r",
							"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\");\r",
							"});\r",
							"\r",
							"pm.test(\"ID in URL matches ID in response\", function () {\r",
							"     const idFromUrl = pm.environment.get(\"CreatedProductId\");\r",
							"     const res = pm.response.json();\r",
							"\r",
							"    pm.expect(res.productId.toString()).to.equal(idFromUrl.toString());\r",
							"});\r",
							"\r",
							"pm.test(\"StockLevel = original + Quantity from request\", function () {\r",
							"    const updated = pm.response.json();\r",
							"\r",
							"    // 1) Hent original produkt (fra GET før PUT)\r",
							"    const originalRaw = pm.environment.get(\"OriginalProduct\");\r",
							"    pm.expect(originalRaw, \"Original product is missing\").to.not.be.undefined;\r",
							"\r",
							"    const original = JSON.parse(originalRaw);\r",
							"\r",
							"    // 2) Parse request body og find Quantity\r",
							"    let delta = 0;\r",
							"\r",
							"    if (pm.request.body && pm.request.body.mode === \"raw\") {\r",
							"        const raw = pm.request.body.raw || \"{}\";\r",
							"        try {\r",
							"            const sent = JSON.parse(raw);\r",
							"\r",
							"            // Tjek både 'Quantity' og 'quantity' for en sikkerheds skyld\r",
							"            delta = Number(sent.Quantity ?? sent.quantity ?? 0);\r",
							"        } catch (e) {\r",
							"            console.log(\"Failed to parse request body as JSON:\", raw, e);\r",
							"        }\r",
							"    }\r",
							"\r",
							"    const originalStock = Number(original.stockLevel);\r",
							"    const expectedStock = originalStock + delta;\r",
							"\r",
							"    console.log(\"Original stockLevel:\", originalStock);\r",
							"    console.log(\"Delta (Quantity) from request:\", delta);\r",
							"    console.log(\"Expected stockLevel:\", expectedStock);\r",
							"    console.log(\"Updated stockLevel:\", updated.stockLevel);\r",
							"\r",
							"    // 3) Assert\r",
							"    pm.expect(\r",
							"        updated.stockLevel,\r",
							"        `stockLevel should be original (${originalStock}) + Quantity (${delta})`\r",
							"    ).to.eql(expectedStock);\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
